beantrage Drucke von Drucker;
beantrage Formatiere von Textverarbeitung;

funk Studium() ergibt Nichts {}
funk Bewerbung() ergibt Zeichenkette { "Hello World!" }

funk Einschreibung(Zahl Matrikelnummer) ergibt Nichts {
